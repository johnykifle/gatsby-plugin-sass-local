{
  "name": "gatsby-plugin-sass-local",
  "description": "Gatsby plugin to handle scss/sass files",
  "version": "0.0.1",
  "author": "Yohannes Russom",
  "bugs": {
    "url": "https://github.com/johnykifle/gatsby-plugin-sass-local/issues"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "gatsby-1-config-css-modules": "^1.0.11",
    "gatsby-1-config-extract-plugin": "^1.0.3",
    "sass": "^1.0.0",
    "sass-loader": "^4.1.1",
    "webpack": "^1.13.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "cross-env": "^5.0.5"
  },
  "homepage": "https://github.com/johnykifle/gatsby-sass-local#readme",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "sass",
    "scss"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^1.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnykifle/gatsby-plugin-sass-local.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore __tests__",
    "prepublish": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore __tests__"
  }
}
